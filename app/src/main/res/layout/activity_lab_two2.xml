<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".soundvibrator.LabTwo">

	<Button android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:id="@+id/vibrator"
	        android:text="vibrate"/>

	<Button android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        app:layout_constraintTop_toBottomOf="@+id/vibrator"
	        android:id="@+id/beeper"
	        android:text="Beeper"/>

	<Button android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:id="@+id/customizedSound"
	        app:layout_constraintTop_toBottomOf="@+id/beeper"
	        android:text="Customized Sounder"/>


</android.support.constraint.ConstraintLayout>